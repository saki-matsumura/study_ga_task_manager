<%# ログイン %>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %><br>
  <br>

  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <br>

  <%= f.submit 'ログイン', class: "btn btn-primary" %><br>
  <br>
<% end %>

<p><%= link_to '新しいアカウントを作る', new_user_path, class: "text-primary"%></p>